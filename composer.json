{
    "name": "akashwttech/laravel-blocker",
    "description": "Laravel Blocker - IP, email, and user blocker package",
    "keywords": [
        "laravel blocker",
        "laravel IP blocker",
        "laravel Email blocker",
        "Email blocker",
        "User blocker",
        "IP blocker",
        "Blocker"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Akash Wttech",
            "email": "akash.p.webtwine@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0|^8.1|^8.2",
        "eklundkristoffer/seedster": "^8.0",
        "laravelcollective/html": "^6.4",
        "illuminate/support": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "jeremykenedy\\LaravelBlocker\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "jeremykenedy\\LaravelBlocker\\LaravelBlockerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
